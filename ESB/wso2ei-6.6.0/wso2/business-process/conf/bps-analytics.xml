<BPSAnalytics xmlns="http://wso2.org/bps/analytics/config">
    <!--Configurations for BPS Analytics-->
    <AnalyticServer>
        <ReceiverURLSet>tcp://localhost:7612</ReceiverURLSet>
        <AuthURLSet>ssl://localhost:7712</AuthURLSet>
        <Username>admin</Username>
        <Password>admin</Password>
    </AnalyticServer>
    <BPMN>
        <!--
        You can change the BPMN data publisher agent type by configuring PublisherType element.
        If not Default Publisher agent type will be used.
        ex. <PublisherType></PublisherType>
        -->
        <DataPublishingEnabled>false</DataPublishingEnabled>
        <KPIDataPublishingEnabled>false</KPIDataPublishingEnabled>
        <AsyncDataPublishingEnabled>false</AsyncDataPublishingEnabled>
    </BPMN>
</BPSAnalytics>